{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=src/.*\\.test\\.(ts|tsx)$",
    "test:unit:watch": "jest --testPathPattern=src/.*\\.test\\.(ts|tsx)$ --watch",
    "test:integration": "jest --testPathPattern=.*\\.integration\\.test\\.(ts|tsx)$",
    "test:api": "jest --testPathPattern=src/app/api/.*\\.test\\.(ts|tsx)$",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:e2e:headless": "cypress run --headless",
    "test:e2e:chrome": "cypress run --browser chrome",
    "test:e2e:firefox": "cypress run --browser firefox",
    "test:security": "jest --testPathPattern=.*security.*\\.test\\.(ts|tsx)$",
    "test:auth": "jest --testPathPattern=.*auth.*\\.test\\.(ts|tsx)$",
    "test:load": "k6 run tests/load/k6-load-tests.js",
    "test:load:local": "BASE_URL=http://localhost:3001 k6 run tests/load/k6-load-tests.js",
    "test:load:staging": "BASE_URL=https://staging.aipromoter.app k6 run tests/load/k6-load-tests.js",
    "test:snapshots": "jest --testPathPattern=.*snapshots.*\\.test\\.(ts|tsx)$",
    "test:snapshots:update": "jest --testPathPattern=.*snapshots.*\\.test\\.(ts|tsx)$ --updateSnapshot",
    "test:db": "jest --testPathPattern=.*database.*\\.test\\.(ts|tsx)$",
    "test:migrations": "jest --testPathPattern=.*migrations.*\\.test\\.(ts|tsx)$",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:security",
    "test:ci": "npm run test:unit && npm run test:integration && npm run test:e2e:headless",
    "test:full": "npm run test:all && npm run test:e2e && npm run test:load:local",
    "analyze": "npm run build:analyze && npx @next/bundle-analyzer .next",
    "perf:audit": "lighthouse --output-path=./lighthouse-report.html --save-assets --view https://localhost:3000",
    "perf:build-size": "npx size-limit",
    "quality:check": "npm run type-check && npm run lint && npm run test:coverage",
    "quality:fix": "npm run lint -- --fix && npm run test:snapshots:update",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:reset": "prisma migrate reset --force",
    "db:studio": "prisma studio",
    "db:seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts",
    "deploy:check": "node scripts/deploy.js check-env",
    "deploy:preview": "node scripts/deploy.js preview",
    "deploy:production": "node scripts/deploy.js production",
    "deploy:status": "node scripts/deploy.js status",
    "vercel:env": "vercel env ls",
    "vercel:logs": "vercel logs",
    "vercel:inspect": "vercel inspect",
    "setup:services": "node scripts/setup-vercel-env.js",
    "services:health": "node scripts/test-services.js"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.859.0",
    "@aws-sdk/lib-storage": "^3.859.0",
    "@hookform/resolvers": "^5.2.1",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toast": "^1.2.14",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@sentry/nextjs": "^10.2.0",
    "@supabase/supabase-js": "^2.53.0",
    "@testing-library/dom": "^10.4.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/multer": "^2.0.0",
    "@types/nodemailer": "^6.4.17",
    "@vercel/analytics": "^1.5.0",
    "@vercel/blob": "^1.1.1",
    "@vercel/kv": "^3.0.0",
    "aws-sdk": "^2.1692.0",
    "bcryptjs": "^3.0.2",
    "@prisma/client": "^6.13.0",
    "axios": "^1.11.0",
    "bullmq": "^5.4.6",
    "compromise": "^14.14.4",
    "crypto-js": "^4.2.0",
    "date-fns": "^2.30.0",
