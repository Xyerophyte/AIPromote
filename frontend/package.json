    "isomorphic-dompurify": "^2.9.0",
    "jsonwebtoken": "^9.0.2",
    "linkedin-api-client": "^0.3.0",
    "moment-timezone": "^0.6.0",
    "natural": "^8.1.0",
    "node-cron": "^4.2.1",
    "openai": "^5.12.0",
    "puppeteer": "^24.16.0",
    "sentiment": "^5.0.2",
    "sharp": "^0.32.6",
    "stripe": "^14.21.0",
    "twitter-api-v2": "^1.24.0",
    "uuid": "^9.0.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.12",
    "jose": "^6.0.12",
    "lucide-react": "^0.536.0",
    "multer": "^2.0.2",
    "next": "15.4.5",
    "next-auth": "^5.0.0-beta.29",
    "nodemailer": "^6.10.1",
    "pdf-parse": "^1.1.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-dropzone": "^14.3.8",
    "react-hook-form": "^7.62.0",
    "react-icons": "^5.5.0",
    "resend": "^5.0.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "web-vitals": "^5.1.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.8",
    "nodemon": "^3.1.10",
    "prisma": "^6.13.0",
    "ts-node": "^10.9.2",
    "autoprefixer": "^10.4.21",
    "cypress": "^13.6.6",
    "eslint": "^9",
    "eslint-config-next": "15.4.5",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.4.1",
    "tw-animate-css": "^1.3.6",
    "typescript": "^5",
    "webpack-bundle-analyzer": "^4.10.1"
  }
}
