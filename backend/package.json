{
  "name": "backend",
  "version": "1.0.0",
  "description": "AI Promote Backend - Fastify server with TypeScript",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/server.ts",
    "dev:worker": "nodemon --exec ts-node src/worker.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "start:worker": "node dist/worker.js",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:seed": "ts-node prisma/seed.ts",
    "db:seed:billing": "ts-node prisma/seed-billing.ts",
    "db:reset": "prisma migrate reset --force",
    "db:studio": "prisma studio",
    "db:backup": "ts-node scripts/backup-database.ts",
    "db:backup:schedule": "ts-node scripts/backup-scheduler.ts",
    "db:health": "ts-node -e \"import('./src/config/database').then(m => m.checkDatabaseHealth()).then(console.log)\"",
    "db:optimize": "ts-node -e \"import('./src/config/database').then(m => m.optimizeIndexes()).then(console.log)\"",
    "db:migrate:manual": "psql $DATABASE_URL -f migrations/manual_add_email_verification_and_optimize_indexes.sql",
    "db:migrate:status": "prisma migrate status",
    "db:format": "prisma format",
    "type-check": "tsc --noEmit",
    "lint": "echo \"Linting not configured yet\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --testPathPattern=integration",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:load": "artillery run tests/load/load-test.yml",
    "test:load:quick": "artillery quick --count 10 --num 5 http://localhost:3000/health",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "coverage:report": "c8 report --reporter=html --reporter=text",
    "coverage:check": "c8 check-coverage --lines 80 --functions 80 --branches 80"
  },
  "keywords": [
    "fastify",
    "typescript",
    "nodejs",
    "api",
    "backend"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.58.0",
    "@aws-sdk/client-s3": "^3.859.0",
    "@aws-sdk/client-secrets-manager": "^3.862.0",
    "@aws-sdk/lib-storage": "^3.859.0",
    "@fastify/cookie": "^11.0.2",
    "@fastify/cors": "^11.1.0",
    "@fastify/helmet": "^13.0.1",
    "@fastify/jwt": "^9.1.0",
    "@fastify/multipart": "^9.0.3",
    "@fastify/oauth2": "^8.1.2",
    "@fastify/rate-limit": "^10.3.0",
    "@prisma/client": "^6.13.0",
    "@sentry/node": "^7.100.0",
    "@sentry/tracing": "^7.100.0",
    "@testing-library/dom": "^10.4.1",
    "@types/multer": "^2.0.0",
    "aws-sdk": "^2.1692.0",
    "axios": "^1.11.0",
    "bcryptjs": "^3.0.2",
    "bullmq": "^5.4.6",
    "compromise": "^14.14.4",
    "crypto-js": "^4.2.0",
    "date-fns": "^2.30.0",
    "dotenv": "^16.6.1",
    "exceljs": "^4.3.0",
    "fastify": "^5.4.0",
    "fastify-plugin": "^5.0.1",
    "fluent-ffmpeg": "^2.1.3",
    "ical-generator": "^9.0.0",
    "ioredis": "^5.3.2",
    "isomorphic-dompurify": "^2.9.0",
    "linkedin-api-client": "^0.3.0",
    "moment-timezone": "^0.6.0",
    "multer": "^2.0.2",
    "natural": "^8.1.0",
    "node-cron": "^4.2.1",
    "nodemailer": "^6.10.1",
    "openai": "^5.12.0",
    "pdf-parse": "^1.1.1",
    "puppeteer": "^24.16.0",
    "sentiment": "^5.0.2",
    "sharp": "^0.32.6",
    "stripe": "^14.21.0",
    "twitter-api-v2": "^1.24.0",
    "uuid": "^9.0.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@cypress/react18": "^2.0.1",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.19.9",
    "@types/nodemailer": "^6.4.17",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.8",
    "artillery": "^2.0.23",
    "c8": "^9.1.0",
    "cypress": "^13.6.6",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "prisma": "^6.13.0",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
